{
  "source": {
    "include": ["public/js/app.js", "server/server.js"],  // Adjust the path to your JS files
    "exclude": ["node_modules"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|tests/)"
  },
  "opts": {
    "destination": "./docs",  // This is where the generated HTML will go
    "recurse": true,
    "readme": "./README.md"
  }
}