{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": true }],
    "no-dupe-keys": "warn",
    "no-dupe-args": "warn",
    "no-unreachable": "warn",
    "no-self-assign": "warn",
    "no-duplicate-imports": "warn"
  }
}